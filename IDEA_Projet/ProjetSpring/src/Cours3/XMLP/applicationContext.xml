<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                            http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Cours3.XMLP -->
    <bean id="person" class="Cours3.XMLP.Person" p:pname="A" p:age="10"
        p:home-ref="homeAddr" p:company-ref="companyAddr">
    </bean>
    <bean id="homeAddr" class="Cours3.XMLP.Adresse" p:addr="AddrA" p:tel="TelA"></bean>
    <bean id="companyAddr" class="Cours3.XMLP.Adresse" p:addr="AddrCA" p:tel="TelCA"></bean>
</beans>